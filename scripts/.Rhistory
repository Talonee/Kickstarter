imports <- read_excel("imports.xlsx")
cols <- c(1, 17:20)
imports <- imports[cols]
kable(imports)
year <- imports$Year
plantFoods <- imports$`Total plant foods`
animalFoods <- imports$`Total animal foods`
beverages <- imports$`Total beverages`
totalFoods <- imports$`Total U.S. food imports`
cat("The mean of the Total foods imports from 1999-2017 = ", mean(totalFoods))
cat("The mean of the Plant foods imports from 1999-2017 = ", mean(plantFoods))
cat("The mean of the Animal foods imports from 1999-2017 = ", mean(animalFoods))
cat("The mean of the beverages imports from 1999-2017 = ", mean(beverages))
plot(totalFoods~year, type = 'l', col = 'red', xlab = "Year", ylab = "U.S. Total Food Imports (Million $)")
legend(1998.75, 130000, legend = c('U.S Total Food Imports'),
col = c('red'), lty = 1)
highestImports <- max(max(plantFoods), max(animalFoods), max(beverages))
plot(plantFoods~year, type = 'l', col = 'dodgerblue4', ylim = c(0, highestImports), xlab = "Year",
ylab = "U.S. Food Imports (Million $)")
lines(animalFoods~year, type = 'l', col = 'darkred')
lines(beverages~year, type = 'l', col = 'darkkhaki')
legend(1998.75, 80000, legend = c('Plant Foods imports', 'Animal Foods imports', 'Beverages imports'),
