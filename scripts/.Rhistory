abline(reg3, col = "orange2")
summary(reg3)
plot(totalFoods~plantFoods, type = 'p', col = "lightsalmon4", xlab = "Plant Foods imports (Million $)", ylab = "U.S. Total Food Imports (Million $)")
reg1 <- lm(totalFoods~plantFoods)
abline(reg1, col = "lightsalmon4")
summary(reg1)
