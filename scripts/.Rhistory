reg2 <- lm(totalFoods~animalFoods)
abline(reg2, col = "lightseagreen")
summary(reg2)
plot(totalFoods~beverages, type = 'p', col = "orange2", xlab = "Beverages imports (Million $)", ylab = "U.S. Total Food Imports (Million $)")
